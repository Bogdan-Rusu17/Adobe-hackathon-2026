{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc",
    "backend-dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "migrate": "knex migrate:latest",
    "rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "db:up": "docker compose up -d"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "axios": "^1.13.2",
    "pg": "^8.16.3",
    "googleapis": "^166.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "express": "^4.19.2",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/knex": "^0.16.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.12.8",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
